Package ecinema{
	abstract class User{
		public string userID;
		public string password;
		
		public string getUserID(){
			return userIDl
		}
		public void setUserID(string ID){
			userID = ID;
		}
		
		public string getPassword(){
			return password
		}
		public void setPassword(string passwd){
			password = passwd;
		}
	}
}

